<resources>
    <declare-styleable name="DoubleSeekBarView">
        <attr name="np_minValue" format="integer"/>
        <attr name="np_maxValue" format="integer"/>
        <attr name="np_defaultValue" format="integer"/>
        <attr name="np_step" format="integer"/>
        <attr name="np_digit" format="integer"/>
        <attr name="np_title" format="string"/>
    </declare-styleable>

    <!-- Reply Preference -->
    <string-array name="reply_entries">
        <item>Reply</item>
        <item>Reply to all</item>
    </string-array>

    <string-array name="reply_values">
        <item>reply</item>
        <item>reply_all</item>
    </string-array>

</resources>